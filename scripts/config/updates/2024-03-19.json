{
    "target": "..\\bundle",
    "ads": {
        "target": "extensions\\ads",
        "data": [
            {
                "name": "Admin Pack for SQL Server",
                "file": "admin-pack-sql-server.vsix",
                "source": "https://go.microsoft.com/fwlink/?linkid=2099889"
            },
            {
                "name": "Database Admin Tool Extensions for Windows",
                "file": "db-admin-tool.vsix",
                "source": "https://go.microsoft.com/fwlink/?linkid=2099888"
            },
            {
                "name": "Query History",
                "file": "query-history.vsix",
                "source": "https://go.microsoft.com/fwlink/?linkid=2109534"
            }
        ]
    },
    "docker": {
        "target": "docker",
        "data": [
            {
                "repository": "nginx",
                "name": "nginx",
                "tag": "latest",
                "clear": false
            },
            {
                "repository": "node",
                "name": "node",
                "tag": "lts-alpine",
                "clear": false
            },
            {
                "repository": "mcr.microsoft.com/dotnet/sdk",
                "name": "mcr.microsoft.com-dotnet-sdk",
                "tag": "latest",
                "clear": false
            },
            {
                "repository": "mcr.microsoft.com/dotnet/aspnet",
                "name": "mcr.microsoft.com-dotnet-aspnet",
                "tag": "latest",
                "clear": false
            },
            {
                "repository": "mcr.microsoft.com/mssql/server",
                "name": "mcr.microsoft.com-mssql-server",
                "tag": "2022-latest",
                "clear": false
            }
        ]
    },
    "dotnet": {
        "target": "dotnet",
        "sdk": {
            "arch": "x64",
            "channel": "LTS",
            "os": "win"
        },
        "tools": {
            "target": "tools",
            "data": [
                "dotnet-ef"
            ]
        }
    },
    "npm": {
        "target": "npm",
        "global": {
            "target": "global",
            "packages": [
                "@angular-devkit/schematics-cli",
                "@angular/cli",
                "@devcontainers/cli",
                "npm"
            ]
        }
    },
    "nuget": {
        "target": "nuget",
        "data": {
            "solution": ".solution",
            "keep": false,
            "clean": true,
            "projects": [
                {
                    "name": "Core",
                    "template": "console",
                    "framework": "net8.0",
                    "dependencies": [
                        "Dapper",
                        "DocumentFormat.OpenXml",
                        "Microsoft.Data.SqlClient",
                        "Microsoft.EntityFrameworkCore",
                        "Microsoft.EntityFrameworkCore.InMemory",
                        "Microsoft.EntityFrameworkCore.Relational",
                        "Microsoft.EntityFrameworkCore.Sqlite",
                        "Microsoft.EntityFrameworkCore.SqlServer",
                        "Microsoft.EntityFrameworkCore.Tools",
                        "Microsoft.Extensions.Configuration.Abstractions",
                        "Microsoft.Extensions.Configuration.Binder",
                        "System.CommandLine!",
                        "System.CommandLine.NamingConventionBinder!"
                    ]
                },
                {
                    "name": "Web",
                    "template": "webapi",
                    "framework": "net8.0",
                    "dependencies": [
                        "Microsoft.AspNetCore.OData",
                        "Microsoft.AspNetCore.OpenApi",
                        "Microsoft.AspNetCore.SignalR.Client",
                        "Microsoft.EntityFrameworkCore.Design",
                        "Microsoft.Identity.Web",
                        "Microsoft.Identity.Web.DownstreamApi",
                        "Swashbuckle.AspNetCore"
                    ]
                }
            ]
        }
    },
    "resources": {
        "target": "resources",
        "files": [
            {
                "name": "Visual Studio Code",
                "file": "vscode.exe",
                "source": "https://code.visualstudio.com/sha/download?build=stable&os=win32-x64-user"
            },
            {
                "name": "git",
                "file": "git.exe",
                "source": "https://github.com/git-for-windows/git/releases/download/v2.44.0.windows.1/Git-2.44.0-64-bit.exe"
            },
            {
                "name": "NodeJS LTS",
                "file": "node.msi",
                "source": "https://nodejs.org/dist/v20.11.1/node-v20.11.1-x64.msi"
            },
            {
                "name": "Azure CLI",
                "file": "azurecli.msi",
                "source": "https://aka.ms/installazurecliwindows"
            },
            {
                "name": "PowerShell",
                "file": "pwsh.msi",
                "source": "https://github.com/PowerShell/PowerShell/releases/download/v7.4.1/PowerShell-7.4.1-win-x64.msi"
            },
            {
                "name": "Azure Data Studio",
                "file": "azure-data-studio.exe",
                "source": "https://go.microsoft.com/fwlink/?linkid=2261569"
            },
            {
                "name": "Docker Desktop",
                "file": "docker.exe",
                "source": "https://desktop.docker.com/win/main/amd64/Docker%20Desktop%20Installer.exe"
            },
            {
                "name": "Docker Buildx",
                "file": "buildx-v0.12.1.windows-amd64.exe",
                "source": "https://github.com/docker/buildx/releases/download/v0.13.1/buildx-v0.13.1.windows-amd64.exe"
            },
            {
                "name": "PowerToys",
                "file": "powertoys.exe",
                "source": "https://github.com/microsoft/PowerToys/releases/download/v0.79.0/PowerToysSetup-0.79.0-x64.exe"
            }
        ]
    },
    "vscode": {
        "target": "extensions\\vscode",
        "data": [
            {
                "publisher": "angular",
                "name": "ng-template",
                "display": "Angular Language Service"
            },
            {
                "publisher": "ms-dotnettools",
                "name": "csharp",
                "display": "C#",
                "targetPlatform": "win32-x64"
            },
            {
                "publisher": "ms-azuretools",
                "name": "vscode-docker",
                "display": "Docker"
            },
            {
                "publisher": "editorconfig",
                "name": "editorconfig",
                "display": "EditorConfig"
            },
            {
                "publisher": "bierner",
                "name": "github-markdown-preview",
                "display": "GitHub Markdown Preview"
            },
            {
                "publisher": "github",
                "name": "github-vscode-theme",
                "display": "GitHub Theme"
            },
            {
                "publisher": "ms-vscode",
                "name": "powershell",
                "display": "PowerShell"
            },
            {
                "publisher": "ms-vscode-remote",
                "name": "vscode-remote-extensionpack",
                "display": "Remote Development"
            },
            {
                "publisher": "PKief",
                "name": "material-icon-theme",
                "display": "Material Icon Theme"
            },
            {
                "publisher": "spmeesseman",
                "name": "vscode-taskexplorer",
                "display": "Task Explorer"
            },
            {
                "publisher": "rangav",
                "name": "vscode-thunder-client",
                "display": "Thunder Client"
            },
            {
                "publisher": "redhat",
                "name": "vscode-yaml",
                "display": "YAML"
            },
            {
                "publisher": "unifiedjs",
                "name": "vscode-mdx",
                "display": "MDX"
            }
        ]
    },
    "wsl": {
        "target": "wsl",
        "arch": "x64"
    }
}